t => current.to === item.to)) {\r\n            return true;\r\n        }\r\n        return false;\r\n    } \r\n    \r\n\r\n    return (\r\n        <li className={open ? \"active\" : \"\"}>\r\n            <a onKeyPress={() => toggle(!open)} onClick={() => toggle(!open)}><i className={`fa ${icon}`}></i> {title} <span className={`fa fa-chevron-${open ? \"up\" : \"down\"}`}></span></a>\r\n            <ul className=\"nav child_menu\" style={open ? {display: 'block'} : {display: 'none'}}>\r\n                {items.map((item, index) => {\r\n                    return (\r\n                        <li key={index} className={isSelectedItem(item) ? \"current-page\" : \"\"} onClick={() => handleOnClick(item)}>\r\n                            <Link to={item.to}>{item.name}</Link>\r\n                        </li>\r\n                    )\r\n                })}\r\n            </ul>\r\n        </li>\r\n    )\r\n}\r\n\r\nexport default Dropdown;\r\n","E:\\Tương tác người máy\\Code\\mgmtvocaapp\\src\\components\\NavTop.js",["189","190","191"],"import React, { Component } from 'react'\r\nimport { Link } from 'react-router-dom'\r\nimport userAvatar from '../utils/images/img.jpg'\r\nimport axios from 'axios'\r\nimport * as API from '../constants/Config'\r\n\r\nconst profileMenus = [\r\n    {\r\n        to: '/user/profile',\r\n        name: \"Profile\",\r\n        icon: \"\",\r\n        type: \"none\"\r\n    },\r\n    {\r\n        to: '/user/settings',\r\n        name: \"Settings\",\r\n        icon: \"fa fa-cogs pull-right\",\r\n        type: \"fa\"\r\n    },\r\n    {\r\n        to: '/user/help',\r\n        name: \"Help\",\r\n        icon: \"\",\r\n        type: \"none\"\r\n    },\r\n    {\r\n        to: '/login',\r\n        name: \"Log Out\",\r\n        icon: \"fa fa-sign-out pull-right\",\r\n        type: \"fa\"\r\n    }\r\n]\r\n\r\nexport class NavTop extends Component {\r\n    constructor(props) {\r\n        super(props)\r\n        this.state = {\r\n            username: '',\r\n      